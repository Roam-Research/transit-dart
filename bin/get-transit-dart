#!/bin/sh

cd `dirname $0`/../..

if [ ! -d "transit-dart" ]; then
  git clone https://github.com/wevre/transit-dart.git
else
  echo "transit-dart exists"
fi

if [ ! -f "transit-dart/resources/emits/roundtrip.exe"]; then
  cd transit-dart
  dart pub get
  mkdir -p resources/emits
  dart compile exe -o resources/emits/roundtrip.exe test/roundtrip.dart
fi